{
  "name": "igreja-oliveira-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "node node_modules/@expo/cli/build/bin/cli start",
    "start:tunnel": "node node_modules/@expo/cli/build/bin/cli start --tunnel",
    "start:tunnel-clear": "node node_modules/@expo/cli/build/bin/cli start --tunnel --clear",
    "start:lan": "node node_modules/@expo/cli/build/bin/cli start --lan",
    "start:clear": "node node_modules/@expo/cli/build/bin/cli start --clear",
    "android": "node node_modules/@expo/cli/build/bin/cli start --android",
    "android:tunnel": "node node_modules/@expo/cli/build/bin/cli start --android --tunnel",
    "ios": "node node_modules/@expo/cli/build/bin/cli start --ios",
    "ios:tunnel": "node node_modules/@expo/cli/build/bin/cli start --ios --tunnel",
    "web": "node node_modules/@expo/cli/build/bin/cli start --web",
    "type-check": "tsc --noEmit",
    "lint": "echo 'ESLint n√£o configurado ainda'",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest --passWithNoTests",
    "build": "node node_modules/@expo/cli/build/bin/cli export",
    "dev": "doppler run -- npm run start",
    "dev:personal": "powershell -Command \"$env:DOPPLER_TOKEN=$env:DOPPLER_TOKEN_PERSONAL; doppler run -- npm run start\"",
    "dev:company": "powershell -Command \"$env:DOPPLER_TOKEN=$env:DOPPLER_TOKEN_COMPANY; doppler run -- npm run start\"",
    "env:check": "doppler run -- node -e \"console.log(!!process.env.SUPABASE_ACCESS_TOKEN)\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@supabase/supabase-js": "^2.50.3",
    "expo": "~53.0.17",
    "expo-blur": "~13.0.2",
    "expo-linear-gradient": "~13.0.2",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "jest": "~29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
